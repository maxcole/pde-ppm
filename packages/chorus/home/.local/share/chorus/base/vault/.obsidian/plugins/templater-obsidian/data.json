{
  "command_timeout": 5,
  "templates_folder": "_config/templates",
  "templates_pairs": [
    [
      "",
      ""
    ]
  ],
  "trigger_on_file_creation": true,
  "auto_jump_to_cursor": true,
  "enable_system_commands": false,
  "shell_path": "",
  "user_scripts_folder": "",
  "enable_folder_templates": false,
  "folder_templates": [
    {
      "folder": "",
      "template": ""
    }
  ],
  "enable_file_templates": true,
  "file_templates": [
    {
      "regex": "products\\/[^/]+\\/$",
      "template": "_config/templates/product.md"
    },
    {
      "regex": "products\\/.*\\/epics\\/",
      "template": "_config/templates/epic.md"
    },
    {
      "regex": "products\\/.*\\/features\\/",
      "template": "_config/templates/feature.md"
    },
    {
      "regex": "products\\/.*\\/stories\\/",
      "template": "_config/templates/story.md"
    },
    {
      "regex": "products\\/.*\\/tasks\\/",
      "template": "_config/templates/task.md"
    }
  ],
  "syntax_highlighting": true,
  "syntax_highlighting_mobile": false,
  "enabled_templates_hotkeys": [
    ""
  ],
  "startup_templates": [
    ""
  ],
  "intellisense_render": 1
}